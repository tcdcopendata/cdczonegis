            <script>
            $(document).ready(function() {
                $('.datepick_one').datepick({ regionalOptions: 'zh-TW' });
                $('.rangePicker').datepick({ rangeSelect: true, regionalOptions: 'zh-TW' });
                $('.blue_qtip').qtip({
                    content: {
                        text: false // Use each elements title attribute
                    },
                    style: 'qtip-blue' // Give it some style
                });
                $('.red_qtip').qtip({
                    content: {
                        text: false // Use each elements title attribute
                    },
                    style: 'qtip-red' // Give it some style
                });
                $('.btn_qtip').qtip({
                    content: {
                        text: false // Use each elements title attribute
                    },
                    position: {
                        at: 'bottom center', // at the bottom right of...
                    },
                    show: {
                        effect: function() {
                            $(this).slideDown();
                        }
                    },
                    hide: {
                        effect: function() {
                            $(this).slideUp();
                        }
                    },
                    style: 'qtip-tipped' // Give it some style
                });
                $('img[title]').qtip({
                    content: {
                        text: false // Use each elements title attribute
                    },
                    style: 'qtip-red' // Give it some style
                });
                $('.check_no_data').qtip({
                    content: {
                        text: false // Use each elements title attribute
                    }
                });
                $('input:radio[name=delivery_choose]').change(function() {
                    if (this.value == 'auto') {
                        $('#hand_delivery').hide();
                        $('#auto_delivery').show();
                    } else if (this.value == 'hand') {
                        $('#hand_delivery').show();
                        $('#auto_delivery').hide();
                    }
                });
            });
            </script>
